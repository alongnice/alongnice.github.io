#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/alongnice.github.io/node_modules/.pnpm/vite@5.3.5_@types+node@20.14.12_lightningcss@1.25.1_sass@1.77.8_stylus@0.63.0_terser@5.31.3/node_modules/vite/bin/node_modules:/workspaces/alongnice.github.io/node_modules/.pnpm/vite@5.3.5_@types+node@20.14.12_lightningcss@1.25.1_sass@1.77.8_stylus@0.63.0_terser@5.31.3/node_modules/vite/node_modules:/workspaces/alongnice.github.io/node_modules/.pnpm/vite@5.3.5_@types+node@20.14.12_lightningcss@1.25.1_sass@1.77.8_stylus@0.63.0_terser@5.31.3/node_modules:/workspaces/alongnice.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/alongnice.github.io/node_modules/.pnpm/vite@5.3.5_@types+node@20.14.12_lightningcss@1.25.1_sass@1.77.8_stylus@0.63.0_terser@5.31.3/node_modules/vite/bin/node_modules:/workspaces/alongnice.github.io/node_modules/.pnpm/vite@5.3.5_@types+node@20.14.12_lightningcss@1.25.1_sass@1.77.8_stylus@0.63.0_terser@5.31.3/node_modules/vite/node_modules:/workspaces/alongnice.github.io/node_modules/.pnpm/vite@5.3.5_@types+node@20.14.12_lightningcss@1.25.1_sass@1.77.8_stylus@0.63.0_terser@5.31.3/node_modules:/workspaces/alongnice.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@5.3.5_@types+node@20.14.12_lightningcss@1.25.1_sass@1.77.8_stylus@0.63.0_terser@5.31.3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@5.3.5_@types+node@20.14.12_lightningcss@1.25.1_sass@1.77.8_stylus@0.63.0_terser@5.31.3/node_modules/vite/bin/vite.js" "$@"
fi
